<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.patient_management_system">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.patient_management_system.Views.ViewTestInfoActivity"></activity>
        <activity android:name="com.example.patient_management_system.Views.UpdateInfoActivity" />
        <activity android:name="com.example.patient_management_system.Views.UpdatePatientActivity" />
        <activity
            android:name="com.example.patient_management_system.Views.AddPatientActivity"
            android:parentActivityName="com.example.patient_management_system.Views.PatientActivity" />
        <activity
            android:name="com.example.patient_management_system.Views.SignUpActivity"
            android:parentActivityName="com.example.patient_management_system.Views.MainActivity" />
        <activity android:name="com.example.patient_management_system.Views.TestActivity" />
        <activity android:name="com.example.patient_management_system.Views.PatientActivity" />
        <activity
            android:name="com.example.patient_management_system.Views.LoginActivity"
            android:parentActivityName="com.example.patient_management_system.Views.MainActivity" />
        <activity android:name="com.example.patient_management_system.Views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>